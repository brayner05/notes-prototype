html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
        link(rel="stylesheet", href="/css/global.css")
        link(rel="stylesheet", href="/css/home.css")
    body 
        main
            .content
                .headings 
                    h1 Notes
                    h2 Open Project
                ul 
                    each project in projects
                        li
                            a(href="notes/" + project.title)=project.title
                button(id="new-project-button") New Project